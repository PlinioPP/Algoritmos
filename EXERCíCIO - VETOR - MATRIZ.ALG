algoritmo "Exercício - VETOR - Matriz"

var

   MA, MB : vetor [1...10] de real
   SOMA : real
   N1, N2 : real

inicio

   SOMA <- 0

   para N1 de 1 ate 10 faca 1
      escreval ("Digite o", N1, "º valor:")
      leia (MA[N1])
   fimpara
   N2 <- N1 / 2
   se (N2 = 0) entao
      escreval

Fimalgoritmo